@model FileModel
@if (Model != null)
{
    <img style="width:100%;" id="#productImage"  itemprop="image" src="@Url.Content("/Files/Sm/" + Model.Name)" alt="@Model.Name" title="@Model.Name" class="zoom-in">
}
else
{
    <img style="width:100%;" itemprop="image" src="@Url.Content("/Files/design/noPhoto.jpg")" alt="@Model.Name" title="@Model.Name" class="zoom-in">
}